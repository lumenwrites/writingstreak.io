  "private": true,
  "scripts": {
    "dev:watch": "next-remote-watch ./*",
    "dev": "next dev -p 3050",
    "develop": "concurrently --kill-others \"npm run dev\" \"npm run processContent:dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate": "npm run copyContent && npm run processContent:prod",
    "copyContent": "rm -rf ./content && mkdir ./content && cp -R ~/Obsidian/Website/godotacademy/ ./content",
    "processContent:prod": "NODE_ENV=production node --experimental-json-modules ./backend/json/processContent.mjs",
    "processContent:dev": "NODE_ENV=development nodemon --experimental-json-modules --watch /Users/ray/Obsidian/Website/godotacademy/ -e mjs,md ./backend/json/processContent.mjs"
  },
